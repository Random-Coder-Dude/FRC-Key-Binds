{
  "name": "frc-key-binds",
  "version": "1.0.0",
  "description": "FRC Key Binder Application",
  "main": "src/electron.js",
  "scripts": {
    "start": "electron .",
    "debug": "electron . --enable-logging",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.example.frc-key-binds",
    "productName": "FRC Key Binds",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "signAndEditExecutable": false
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "express": "^4.21.2"
  },
  "devDependencies": {
    "electron": "^35.1.4",
    "electron-builder": "^24.6.0",
    "sass": "^1.86.3"
  }
}
